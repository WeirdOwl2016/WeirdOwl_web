{
  "name": "newroadAdmin",
  "version": "1.0.0",
  "description": "newroadAdmin project",
  "author": "hyh",
  "private": true,
  "lint": "eslint",
  "scripts": {
    "build:client": "cross-env CONFIG_PATH=F:/模板/web/config.json NODE_ENV=development  webpack --colors --config ./build/webpack.client.config.js",
    "build:server": "cross-env CONFIG_PATH=F:/模板/web/config.json NODE_ENV=development  webpack --colors --config ./build/webpack.server.config.js",
    "dev": " cross-env CONFIG_PATH=F:/模板/web/config.json  NODE_ENV=development  node web.js",
    "start": "cross-env NODE_ENV=production CONFIG_PATH=F:/模板/web/config.json  node web.js ",
    "build": "npm run unzip && npm run permission && npm run build:client && npm run  build:server && npm run tar",
    "unzip": "unzip -o -q node_modules.zip",
    "permission": "chmod -R 777 node_modules",
    "tar": "tar czvf ../newRoadAdmin.tar * --exclude=node_modules.zip --exclude=gulp  --exclude=test/api --exclude=test/helpers --exclude=src/web --exclude=.svn"
  },
